---
import Text from './Text.astro';

const { heading, subheading } = Astro.props;
---

<>
  <div class="flex flex-col gap-y-3">
    <Text type="h1">{heading}</Text>
    <Text type="sh1">{subheading}</Text>
  </div>
  <div class="pt-9"></div>
  <div class="flex flex-row gap-x-[60px]">
    <slot />
  </div>
</>
